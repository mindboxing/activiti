<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.mindboxing.org/schemas/itinerary" targetNamespace="http://www.mindboxing.org/schemas/itinerary" elementFormDefault="qualified">

	<xs:complexType name="SegmentType">
		<xs:annotation>
			<xs:documentation>This is data that is common for every segment
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Origination" type="TravelPointType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						This is the starting point for a segment.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Destination" type="TravelPointType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						This is the landing place.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillingCode" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						This is the code that determines how the billing
						information is sent.
					</xs:documentation>
				</xs:annotation>
			</xs:element>


			<xs:element name="ClassOfService" type="ClassOfServiceType" maxOccurs="1" minOccurs="0">
			</xs:element>
			<xs:element name="Commisson" type="CommissonType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						An additional amount of money that is given due
						to referrals.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Confirmation" type="xs:string" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The validation of information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConventionCode" type="xs:string" maxOccurs="1" minOccurs="0">
			</xs:element>
			<xs:element name="Equipment" type="EquipmentType" maxOccurs="1" minOccurs="0">
			</xs:element>


			<xs:element name="Guarantee" type="GuaranteeType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The pledge or promise of information or action.
					</xs:documentation>
					<xs:documentation>
						Indicates the guarantee type.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InvoiceRemark" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The remark, or free form field of information,
						in
						an invoice.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Memberships" type="xs:string" maxOccurs="unbounded" minOccurs="0">

				<xs:annotation>
					<xs:documentation>
						Having loyalty or association with a company.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReferenceTravelerName" type="xs:string" maxOccurs="1" minOccurs="0">
			</xs:element>
			<xs:element name="ReservedSpace" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates that a seat or position is reserved.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialInformation" type="SpecialInfoType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						A field for free form information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TourCode" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Code for indicating tour information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Vendor" type="VendorType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						A field for merchant information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Discounts" type="DiscountType" maxOccurs="unbounded" minOccurs="0">
				<!-- <xs:annotation> <xs:documentation> Indicates a type of discount 
					or special offer. </xs:documentation> </xs:annotation> -->
				<xs:annotation>
					<xs:documentation>
						A field for discount and special offer
						information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Seats" type="SeatType" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates the seat or space on a mode of
						transporation.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WrittenConfirmationNeeded" type="xs:boolean" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates that a notice of verification must be
						included.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remarks" type="xs:string" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						A field for extra free form information.
					</xs:documentation>
					<xs:documentation>
						A field for extra free form information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FreeText" type="xs:string" maxOccurs="unbounded" minOccurs="0"/>
			<xs:element name="Phones" type="xs:string" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates phone information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice maxOccurs="1" minOccurs="1">
				<xs:element name="Air" type="AirSegmentType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Indicates airline information.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Car" type="CarSegmentType" maxOccurs="1" minOccurs="0">
					<!-- <xs:annotation> <xs:documentation> This segment type is used for 
						Car segments. </xs:documentation> </xs:annotation> -->
					<xs:annotation>
						<xs:documentation>
							Indicates car information.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Hotel" type="HotelSegmentType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							This segment type is used for Hotel
							Segments.
						</xs:documentation>
						<xs:documentation>
							Indicates hotel information.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Surface" type="SurfaceSegmentType" maxOccurs="1" minOccurs="0">
				</xs:element>
				<xs:element name="Tour" type="TourSegmentType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Indicates tour information.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Rail" type="RailSegmentType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Indicates railway information.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Bus" type="RailSegmentType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Indicates bus information.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Sea" type="SeaSegmentType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Indicates boat and cruise information.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Limo" type="TourSegmentType" maxOccurs="1" minOccurs="0"/>
				<xs:element name="Other" type="OtherSegmentType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							A free form field for information that is
							not fulfilled in the already assigned
							categories/types.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>

		</xs:sequence>

		<xs:attribute name="duration" type="xs:string">
			<xs:annotation>
				<xs:documentation> Indicates an amount in minutes
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="quantity" type="xs:int">
			<xs:annotation>
				<xs:documentation> Indicates an amount of something.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="routeId" type="xs:string">
			<xs:annotation>
				<xs:documentation>Flight, train, bus, tour number</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="segmentNbr" type="xs:int">
			<xs:annotation>
				<xs:documentation> Indicates the identifying number of the segment.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="segmentStatus" type="xs:string">
			<xs:annotation>
				<xs:documentation> Indicates the status of the segment.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type">
			<xs:annotation>
				<xs:documentation>
					Indicates the segment type.
				</xs:documentation>
			</xs:annotation>

			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="AIR"/>
					<xs:enumeration value="HOTEL"/>
					<xs:enumeration value="CAR"/>
					<xs:enumeration value="TOUR"/>
					<xs:enumeration value="LIMO"/>
					<xs:enumeration value="RAIL"/>
					<xs:enumeration value="SEA"/>
					<xs:enumeration value="BUS"/>					
					<xs:enumeration value="SURFACE"/>
					<xs:enumeration value="MCO"/>
					<xs:enumeration value="INS"/>
					<xs:enumeration value="OTHER"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>

	</xs:complexType>

	<xs:complexType name="CommissonType">
		<xs:annotation>
			<xs:documentation> Indicates the commission type.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Amount" type="CurrencyAmountType" maxOccurs="1" minOccurs="1">
				<xs:annotation>
					<xs:documentation> Indicates quantity.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="code" type="xs:string"/>
	</xs:complexType>

	<xs:complexType name="TravelPointType">
		<xs:annotation>
			<xs:documentation>
				Indicates the type of travel point or destination/stop.
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="cityCode" type="xs:string">
			<xs:annotation>
				<xs:documentation>
					Indicates the code identifier for a city.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="dayOfWeek" type="xs:string">
			<xs:annotation>
				<xs:documentation>
					Indicates the day of the week.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="date" type="xs:string">
			<xs:annotation>
				<xs:documentation>Indicates the date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="time" type="xs:string">
			<xs:annotation>
				<xs:documentation>Indicates the time.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="dateTime" type="xs:dateTime"/>
		<xs:attribute name="cityDescription" type="xs:string">
			<xs:annotation>
				<xs:documentation>
					A field of text describing a city.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>

		<xs:attribute name="countryCode" type="xs:string"/>
		<xs:attribute name="terminal" type="xs:string">
			<xs:annotation>
				<xs:documentation>
					Indicates the terminal, or transportation boarding
					site.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="gate" type="xs:string">
			<xs:annotation>
				<xs:documentation>
					Indicates the location of the gate, or
					transportation boarding site.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="freeText" type="xs:string"/>
		<xs:attribute name="checkInTime" type="xs:string"/>
	</xs:complexType>



	<xs:complexType name="RateType">
		<xs:annotation>
			<xs:documentation>
				Indicates the type of rate.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Remark" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						A field for extra free form information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Amount" type="CurrencyAmountType">
				<xs:annotation>
					<xs:documentation>
						Indicates a quantitiy.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateTermCode" type="rateTermCode"/>
		</xs:sequence>
		<xs:attribute name="code" type="xs:string">


		</xs:attribute>
		<xs:attribute name="description" type="xs:string"/>

	</xs:complexType>

	<xs:complexType name="SpecialInfoType">
		<xs:annotation>
			<xs:documentation> Indicates the type of field for special
				information.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Equipment" type="xs:string" maxOccurs="unbounded" minOccurs="0">
			</xs:element>
			<xs:element name="Information" type="xs:string" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation> Indicates where general information is located.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Request" type="xs:string" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DiscountType">
		<xs:attribute name="type" type="xs:string"/>
		<xs:attribute name="corporateId" type="xs:string">
			<xs:annotation>
				<xs:documentation> Indicates an identifier for a corporation.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="vendorId" type="xs:string">
			<xs:annotation>
				<xs:documentation> Indicates an identifier for a vendor.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value" type="xs:string">
			<xs:annotation>
				<xs:documentation> Indicates an amount of something.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>


	<xs:complexType name="AirSegmentType">
		<xs:annotation>
			<xs:documentation>
				This segment type is used for Air and Rail
				Segments.
				Rail Segments are entered as Air Segments in the GDS
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightMiles" type="xs:int" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						A field where airline miles are entered.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Meal" type="MealType" maxOccurs="1" minOccurs="0"/>
			<xs:element name="Entertainment" type="xs:string" maxOccurs="1" minOccurs="0">
			</xs:element>
			<xs:element name="SmokingIndicator" type="xs:boolean" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates that the traveler is a smoker.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndDateAdjustment" type="xs:int" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Changes the return date.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AllianceInformation" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Information about the customer/company's
						loyalties to certain transportation/hotel/etc
						companies.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StopCities" type="SegmentType" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates the cities where the traveler will be
						departing from transportation, and possibly
						transferring.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StopDuration" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates how long the span of time between when
						the traveler departs and boards transportation.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OperatedBy" type="VendorType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates what company the transportation is
						operated by.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Fare" type="FareAmountType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						This is the TDC Authorizer fare amount type for
						the segment.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="open" type="xs:boolean"/>

		<xs:attribute name="eTicket" type="xs:boolean">
			<xs:annotation>
				<xs:documentation> Indicates an electroic ticket.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>


	<xs:complexType name="RailSegmentType">
		<xs:annotation>
			<xs:documentation>
				This segment type is used for Air and Rail
				Segments.
				Rail Segments are entered as Air Segments in the GDS
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TripMiles" type="xs:int">
				<xs:annotation>
					<xs:documentation>
						A field where airline miles are entered.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Meal" type="MealType">
				<xs:annotation>
					<xs:documentation>
						Indicates a type of meal to be served.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SmokingIndicator" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Indicates that the traveler is a smoker.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndDateAdjustment" type="xs:int">
				<xs:annotation>
					<xs:documentation>
						Changes the return date.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AllianceInformation" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Information about the customer/company's
						loyalties to certain transportation/hotel/etc
						companies.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StopCities" type="SegmentType" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates the cities where the traveler will be
						departing from transportation, and possibly
						transferring.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StopDuration" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates how long the span of time between when
						the traveler departs and boards transportation.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OperatedBy" type="VendorType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates what company the transportation is
						operated by.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TicketControlNumber" type="xs:string" maxOccurs="unbounded" minOccurs="0">
			</xs:element>
			<xs:element name="Fare" type="FareAmountType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						This is the TDC Authorizer fare amount type for
						the segment.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="open" type="xs:boolean"/>

	</xs:complexType>


	<xs:complexType name="CarSegmentType">
		<xs:annotation>
			<xs:documentation> This segment type is used for Car segments.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RatesAndFees" type="CarChargeType" maxOccurs="unbounded" minOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Indicates the type of fees for rental cars.
					</xs:documentation>
					<xs:documentation>
						A field designated for information pertaining to
						rates and fees.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PickupNote" type="xs:string" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						A field for remarks about picking up the
						traveler
						at a destination.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="xs:string" maxOccurs="1" minOccurs="0">
			</xs:element>
			<xs:element name="RentalRateCode" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates the rate for car rentals.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateQuote" type="xs:string" maxOccurs="1" minOccurs="0"/>
			<xs:element name="FlightInfo" type="FlightInfoType" maxOccurs="1" minOccurs="0">
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="HotelSegmentType">
		<xs:annotation>
			<xs:documentation> This segment type is used for Hotel Segments.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Address" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Indicates the type of address.
					</xs:documentation>
					<xs:documentation>
						A field for address information.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalDetails" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						A field for remarks about the arrival point.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureDetails" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						A field for remarks about the depature point.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateQuote" type="xs:string" maxOccurs="1" minOccurs="0">
			</xs:element>
